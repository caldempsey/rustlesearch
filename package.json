{
  "name": "rustlesearch-server",
  "version": "0.0.1",
  "description": "Server for rustlesearch ingest & web api",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "scripts": {
    "pretty": "prettier --loglevel warn --write '**/*.{js,yml,json}' && eslint --ext .js --fix ./",
    "start:app": "node --require dotenv/config index.js",
    "start:download": "node --require dotenv/config ./scripts/downloadFiles.js",
    "start:index": "node --require dotenv/config ./scripts/indexToElasticsearch.js",
    "start:benchmark": "node --require dotenv/config ./scripts/benchmarkIngest.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty"
    }
  },
  "contributors": [
    {
      "email": "johnpaulpenaloza@gmail.com",
      "name": "John P. Penaloza",
      "url": "https://johnpyp.com"
    },
    {
      "email": "48770555+alg1142@users.noreply.github.com",
      "name": "alice lilith iris eleonore gladwyn",
      "url": "https://ayaya.dev"
    }
  ],
  "_moduleAliases": {
    "@root": ".",
    "@src": "./src",
    "@lib": "./src/lib",
    "@routes": "./src/routes"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.1.0",
    "bluebird": "3.5.5",
    "date-fns": "1.30.1",
    "dotenv": "8.0.0",
    "etl": "0.6.3",
    "fast-glob": "2.2.7",
    "fastify": "2.4.1",
    "fastify-cors": "2.1.3",
    "fastify-helmet": "3.0.0",
    "fastify-rate-limit": "2.1.1",
    "lodash": "4.17.11",
    "module-alias": "2.2.0",
    "node-etl": "1.0.3",
    "pino": "5.12.6",
    "pino-pretty": "3.1.0",
    "ramda": "0.26.1",
    "request": "2.88.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.4.0",
    "prettier": "1.18.2",
    "yaml": "1.6.0",
    "yaml-unist-parser": "1.0.0"
  }
}
