{
  "name": "rustlesearch-server",
  "version": "0.0.1",
  "description": "Server for rustlesearch ingest & web api",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "scripts": {
    "pretty": "prettier --loglevel warn --write '**/*.js' && eslint --ext .js --fix ./",
    "start:app": "node --require dotenv/config index.js",
    "start:download": "node --require dotenv/config ./scripts/downloadFiles.js",
    "start:index": "node --require dotenv/config ./scripts/indexToElasticsearch.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty"
    }
  },
  "contributors": [
    {
      "email": "johnpaulpenaloza@gmail.com",
      "name": "John P. Penaloza",
      "url": "https://johnpyp.com"
    },
    {
      "email": "48770555+alg1142@users.noreply.github.com",
      "name": "alice lilith iris eleonore gladwyn",
      "url": "https://ayaya.dev"
    }
  ],
  "dependencies": {
    "@elastic/elasticsearch": "7.1.0",
    "cors": "2.8.5",
    "date-fns": "1.30.1",
    "dotenv": "8.0.0",
    "etl": "0.6.3",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "express-winston": "3.1.0",
    "fast-glob": "2.2.7",
    "lodash": "4.17.11",
    "node-etl": "1.0.3",
    "ramda": "0.26.1",
    "request": "2.88.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.4.0",
    "prettier": "1.17.1"
  }
}
