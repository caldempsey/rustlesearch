{
  "name": "rustlesearch-server",
  "version": "0.0.1",
  "description": "Server for rustlesearch ingest & web api",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "scripts": {
    "pretty": "prettier --loglevel warn --write '**/*.{js,yml,json}' && eslint --ext .js --fix ./",
    "test": "jest --forceExit",
    "start:app": "node --require @babel/register --require dotenv/config src/server.js",
    "start:app:production": "node --require dotenv/config dist/server.js",
    "build": "babel -d ./dist ./src --copy-files --delete-dir-on-start",
    "run:download": "node --require dotenv/config ./scripts/downloadFiles.js",
    "run:index": "node --require @babel/register --require dotenv/config ./scripts/indexToElasticsearch.js",
    "benchmark:index": "node --require dotenv/config ./scripts/benchmarkIngest.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty"
    }
  },
  "contributors": [
    {
      "email": "johnpaulpenaloza@gmail.com",
      "name": "John P. Penaloza",
      "url": "https://johnpyp.com"
    },
    {
      "email": "48770555+alg1142@users.noreply.github.com",
      "name": "alice lilith iris eleonore gladwyn",
      "url": "https://ayaya.dev"
    }
  ],
  "_moduleAliases": {
    "@root": ".",
    "@src": "./src",
    "@lib": "./src/lib",
    "@routes": "./src/routes"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.1.0",
    "bluebird": "3.5.5",
    "date-fns": "1.30.1",
    "dotenv": "8.0.0",
    "etl": "0.6.3",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "express-rate-limit": "5.0.0",
    "fast-glob": "2.2.7",
    "helmet": "3.18.0",
    "lodash": "4.17.11",
    "luxon": "1.16.0",
    "module-alias": "2.2.0",
    "node-etl": "1.0.3",
    "pino": "5.12.6",
    "pino-pretty": "3.1.0",
    "ramda": "0.26.1",
    "request": "2.88.0",
    "supertest": "4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/parser": "7.4.5",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.4.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "jest-raw-loader": "1.0.1",
    "prettier": "1.18.2",
    "yaml": "1.6.0",
    "yaml-unist-parser": "1.0.0"
  }
}
